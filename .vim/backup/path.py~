"""
    ADMIN USERS
"""
def admin_users():
    return '/admin/users'

from django.conf import settings

"""
"""
def admin_users_new():
    return '/admin/users/new'

"""
"""
def admin_users_show(user_id):
    return '/admin/users/'+str(user_id)

"""
"""
def admin_users_edit(user_id):
    return '/admin/users/edit/'+str(user_id)

"""
"""
